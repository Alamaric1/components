    method public void renderTabs()

        if #Tabs! <> null() then
            #Tabs!.destroy()
            #Tabs! = null()
        fi

        #Tabs! = new DockingTabsComponent(#X+4,#Y+4,#W-4,32,#this!, #FrameSet!)


