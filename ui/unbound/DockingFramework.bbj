    method public void resetAll(FrameList fl!)


        if #Tabs! <> null() then
            #Tabs!.destroy()
            #Tabs! = null()
        fi

        #this!.removeSplitterPaneChildWindows()

        if #size() then

            for i=0 to #size()-1

                if #ContentType=2 then
                    fl!.addItem(#getItem(i))
                else
                    e! = #getItem(i)
                    e!.resetAll(fl!)
                fi

            next

        fi

    methodend

    method public void notifySelectionChange(BBjNumber id)

        if #size() then

            for i=0 to #size()-1
                cell! = #getItem(i)
                cell!.setVisible(i= #Tabs!.getSelectedTabId())
            next

        fi

    methodend

    method public void removeFrameCell(FrameCell fc!)


        c=#size()

        if c=0 then methodret

        if #ContentType = 2 then

            for i=0 to c-1
                f! = #getItem(i)

                if f! = fc! then
                    #removeItem(i)

                    if #size() = 0 then
                        #Tabs!.destroy()
                        #Tabs! = null()
                    fi


                    methodret

                fi

            next


        else

            for i=0 to c-1
                f! = #getItem(i)
                f!.removeFrameCell(fc!)
            next

        fi


    methodend

